{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "api/v1/route.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ui/ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/route.js"
    },
    {
      "src": "/(.*)",
      "dest": "/ui/ui/$1"
    }
  ]
}
